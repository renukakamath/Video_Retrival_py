<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.videoretrival">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.VideoRetrival"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">
        <activity
            android:name=".Videos"
            android:exported="false" />
        <activity
            android:name=".VideoPlay"
            android:exported="false" />
        <activity
            android:name=".Sendcomplaint"
            android:exported="false" />
        <activity
            android:name=".Viewnotes"
            android:exported="false" />
        <activity
            android:name=".Searchkeyword"
            android:exported="false" />
        <activity
            android:name=".Viewsubject"
            android:exported="false" />
        <activity
            android:name=".Updateprofile"
            android:exported="false" />
        <activity
            android:name=".Userhome"
            android:exported="false" />
        <activity
            android:name=".login"
            android:exported="false" />
        <activity
            android:name=".Ipsettings"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>